n    padding: 0;\r\n    cursor: pointer;\r\n\r\n    &:checked + label > span {\r\n      border: 9px solid;\r\n      animation-name: pulse;\r\n      animation-duration: 1.3s;\r\n    }\r\n\r\n    &:checked + label > span:before {\r\n      content: '';\r\n      position: absolute;\r\n      bottom: 0;\r\n      right: 0;\r\n      border-bottom: 2px solid transparent;\r\n      border-right: 2px solid transparent;\r\n      transform: rotate(45deg);\r\n      transform-origin: 0 100%;\r\n      animation-name: checkbox-check;\r\n      animation-duration: 125ms;\r\n      animation-delay: 250ms;\r\n      animation-timing-function: cubic-bezier(.4, .0, .23, 1);\r\n      animation-fill-mode: forwards;\r\n      //animation: checkbox-check 125ms 250ms cubic-bezier(.4, .0, .23, 1) forwards;\r\n    }\r\n  }\r\n\r\n  label {\r\n    position: relative;\r\n    font-weight: normal;\r\n    font-size: .8rem;\r\n    text-indent: 2rem;\r\n\r\n    > span {\r\n      position: absolute;\r\n      width: 18px;\r\n      height: 18px;\r\n      border-radius: 50%;\r\n      border: 1px solid;\r\n      z-index: 0;\r\n      left: -2px;\r\n      top: -2px;\r\n      background-color: transparent;\r\n      transition: all 250ms cubic-bezier(.4, .0, .23, 1);\r\n      animation-name: pulse2;\r\n      animation-duration: 1.3s;\r\n    }\r\n  }\r\n}\r\n\r\n@keyframes pulse {\r\n  0% {\r\n    box-shadow: 0 0 0 0 rgba(230, 236, 240, 1);\r\n  }\r\n  50% {\r\n    box-shadow: 0 0 0 35px rgba(230, 236, 240, 0);\r\n  }\r\n  100% {\r\n    box-shadow: 0 0 0 0 rgba(230, 236, 240, 0);\r\n  }\r\n}\r\n\r\n@keyframes pulse2 {\r\n  0% {\r\n    box-shadow: 0 0 0 0 rgba(230, 236, 240, 1);\r\n  }\r\n  50% {\r\n    box-shadow: 0 0 0 35px rgba(230, 236, 240, 0);\r\n  }\r\n  100% {\r\n    box-shadow: 0 0 0 0 rgba(230, 236, 240, 0);\r\n  }\r\n}\r\n\r\n@keyframes checkbox-check {\r\n  0% {\r\n    border-color: #fff;\r\n  }\r\n  33% {\r\n    width: 4px;\r\n    height: 0;\r\n  }\r\n  100% {\r\n    width: 4px;\r\n    height: 8px;\r\n    border-color: #fff;\r\n  }\r\n}","styles\\index.scss":"@import \"checkbox\";\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n  font-family: 'Roboto', sans-serif;\r\n}\r\n\r\n@mixin clear-list {\r\n  list-style: none;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n.app-title {\r\n  text-align: center;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\nbody {\r\n  padding-top: 3rem;\r\n  padding-bottom: 3rem;\r\n}\r\n\r\n.theme-button {\r\n  display: flex;\r\n  justify-content: center;\r\n\r\n  a {\r\n    color: #36a8f1;\r\n    text-decoration: none;\r\n    font-size: 1.2rem;\r\n    font-weight: normal;\r\n  }\r\n}\r\n\r\n.chart-wrap {\r\n  display: flex;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  padding-top: 2rem;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\nbody.tg-chart-preload .tg-chart-checkbox  {\r\n  & span, & span:before {\r\n    animation-duration: 0s !important;\r\n    transition: border 0s !important;\r\n  }\r\n}\r\n\r\nbody {\r\n  transition: background .22s ease-in-out;\r\n}\r\n\r\nbody.tg-night {\r\n  background: #242f3e;\r\n\r\n  .app-title {\r\n    color: #fff;\r\n  }\r\n}\r\n\r\n.tg-chart {\r\n  position: relative;\r\n\r\n  > canvas {\r\n    z-index: 2;\r\n  }\r\n\r\n  .tg-chart-slider {\r\n    position: relative;\r\n    margin-bottom: 1rem;\r\n    z-index: 1;\r\n\r\n    &__right, &__left {\r\n      transition: background .22s ease-in-out;\r\n      background: #f5f9fb;\r\n      opacity: .8;\r\n      position: absolute;\r\n      top: 0;\r\n      bottom: 0;\r\n    }\r\n\r\n    &__left {\r\n      left: 0;\r\n    }\r\n\r\n    &__right {\r\n      right: 0;\r\n    }\r\n\r\n    &__arrow--left, &__arrow--right {\r\n      position: absolute;\r\n      top: 0;\r\n      bottom: 0;\r\n      transition: background .22s ease-in-out;\r\n      background: #ddeaf3;\r\n      width: 4px;\r\n    }\r\n\r\n    &__arrow--left {\r\n      right: 0;\r\n      cursor: w-resize;\r\n    }\r\n\r\n    &__arrow--right {\r\n      left: 0;\r\n      cursor: e-resize;\r\n    }\r\n\r\n    &__window {\r\n      position: absolute;\r\n      background: transparent;\r\n      top: 0;\r\n      bottom: 0;\r\n    }\r\n  }\r\n\r\n  .tg-chart-tooltip {\r\n    position: absolute;\r\n    display: none;\r\n    max-width: 200px;\r\n    min-width: 120px;\r\n    padding: 0.5rem;\r\n    border-radius: 5px;\r\n    border: 1px solid #dfe6eb;\r\n    box-shadow: 1px 1px 1px rgba(0,0,0,.05);\r\n    background: #fff;\r\n    color: #000;\r\n    overflow: hidden;\r\n    z-index: 3;\r\n\r\n    top: 50px;\r\n    left: 100px;\r\n\r\n    .tooltip-title {\r\n      text-align: center;\r\n      margin-bottom: .5rem;\r\n      white-space: nowrap;\r\n    }\r\n\r\n    .tooltip-list {\r\n      display: flex;\r\n      flex-wrap: wrap;\r\n      @include clear-list;\r\n    }\r\n\r\n    .tooltip-list-item {\r\n      display: flex;\r\n      flex-direction: column;\r\n      align-items: center;\r\n      flex-grow: 1;\r\n\r\n      .name {\r\n        font-size: .8rem;\r\n      }\r\n\r\n      .value {\r\n        font-size: 1rem;\r\n        font-weight: bold;\r\n      }\r\n    }\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"a7e32e66646dfc98f1a276166ad2cc00","cacheData":{"env":{}}}